defines = -DNDEBUG -DSK_ASSUME_WEBGL=1
framework_dirs =
include_dirs = -I../../../../root/skia
cflags = -Werror -Wno-attributes -fstrict-aliasing -fPIC -fvisibility=hidden -O3 -fdata-sections -ffunction-sections -Wno-sign-conversion -Wno-unused-parameter -s WARN_UNALIGNED=1 -s MAIN_MODULE=1 -DSKNX_NO_SIMD -DSK_DISABLE_AAA -DSK_DISABLE_LEGACY_SHADERCONTEXT -DSK_RELEASE -DGR_GL_CHECK_ALLOC_WITH_GET_ERROR=0
cflags_cc = -std=c++17 -fvisibility-inlines-hidden -fno-exceptions -fno-rtti -frtti
root_out_dir = .
target_output_name = libdebugcanvas

build obj/tools/libdebugcanvas.SkSharingProc.o: cxx ../../../../root/skia/tools/SkSharingProc.cpp
build obj/tools/libdebugcanvas.UrlDataManager.o: cxx ../../../../root/skia/tools/UrlDataManager.cpp
build obj/tools/debugger/libdebugcanvas.DebugCanvas.o: cxx ../../../../root/skia/tools/debugger/DebugCanvas.cpp
build obj/tools/debugger/libdebugcanvas.DebugLayerManager.o: cxx ../../../../root/skia/tools/debugger/DebugLayerManager.cpp
build obj/tools/debugger/libdebugcanvas.DrawCommand.o: cxx ../../../../root/skia/tools/debugger/DrawCommand.cpp
build obj/tools/debugger/libdebugcanvas.JsonWriteBuffer.o: cxx ../../../../root/skia/tools/debugger/JsonWriteBuffer.cpp

build ./libdebugcanvas.a: alink obj/tools/libdebugcanvas.SkSharingProc.o obj/tools/libdebugcanvas.UrlDataManager.o obj/tools/debugger/libdebugcanvas.DebugCanvas.o obj/tools/debugger/libdebugcanvas.DebugLayerManager.o obj/tools/debugger/libdebugcanvas.DrawCommand.o obj/tools/debugger/libdebugcanvas.JsonWriteBuffer.o
  arflags =
  output_extension = .a
  output_dir = 
